{	// launch.json
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [		

		{
	    	"name": "Debug",
			"cwd": "${workspaceFolder}",
			"executable": "./src/learn.elf",
			"request": "launch",
			"type": "cortex-debug",
			"runToEntryPoint": "main",
			"servertype": "stutil",
			"device": "STM32F413ZH",
			"interface": "swd",
			"preLaunchTask": "build"
		},
		{
			"name": "Attach",
			"cwd": "${workspaceFolder}/src",
			"executable": "./learn.elf",
			"request": "attach",
			"type": "cortex-debug",
			"runToEntryPoint": "main",
			"servertype": "stutil",
			"device": "STM32F413ZH",
			"interface": "swd"
		},
	]
}